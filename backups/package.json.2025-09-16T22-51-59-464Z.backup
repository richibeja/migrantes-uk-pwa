    "build": "next build",
    "build:production": "node scripts/build-production.js",
    "build:vercel": "npm run clean && next build --debug",
    "start": "next start",
    "start:production": "NODE_ENV=production next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "Remove-Item -Recurse -Force .next, node_modules, package-lock.json -ErrorAction SilentlyContinue",
    "clean:all": "Remove-Item -Recurse -Force .next, out, dist, build, node_modules, package-lock.json -ErrorAction SilentlyContinue",
    "vercel-build": "npm install --legacy-peer-deps && npm run build",
    "vercel-dev": "npm install --legacy-peer-deps && npm run dev",
